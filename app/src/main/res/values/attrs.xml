<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiSelectListPreference">
        <attr name="android:entries" />
        <attr name="android:entryValues" />
    </declare-styleable>

    <declare-styleable name="MoreAppsCard">
        <attr name="CardTitle" format="string" />
        <attr name="AppsCategory" format="string" />
        <attr name="JsonName" format="string" />
    </declare-styleable>

    <declare-styleable name="TagView">
        <attr name="TagStyle" format="enum">
            <enum name="Mono" value="0" />
            <enum name="Duo" value="1" />
        </attr>
        <attr name="MonoTagName" format="string" />
        <attr name="DualTagName0" format="string" />
        <attr name="DualTagName1" format="string" />
    </declare-styleable>

    <declare-styleable name="AdaptiveToolbar">
        <attr name="ToolbarStyle" format="enum">
            <enum name="Home" value="0" />
            <enum name="Details" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FilterBadge">
        <attr name="badge_title" format="string" />
        <attr name="badge_dotColor" format="integer" />
        <attr name="badge_filter_key" format="string" />
    </declare-styleable>

    <declare-styleable name="RoundedLinearLayout">
        <attr name="customLRadius" format="dimension" />
        <attr name="customLShadow" format="dimension" />
        <attr name="customLBackgroundColor" format="color" />
        <attr name="customLShadowColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RoundedRelativeLayout">
        <attr name="customRRadius" format="dimension" />
        <attr name="customRShadow" format="dimension" />
        <attr name="customRBackgroundColor" format="color" />
        <attr name="customRShadowColor" format="color" />
    </declare-styleable>
</resources>
